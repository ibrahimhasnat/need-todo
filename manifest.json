{
    "manifest_version": 2,
    "name": "Need Todo",
    "description": "Simple Todo Extension",
    "version": "0.1.0",
    "icons": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "app.html"
    },
    "permissions": ["activeTab"]
}